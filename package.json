{
  "name": "action-install-minepkg",
  "version": "0.0.1",
  "private": true,
  "description": "Installs the minepkg CLI, so you can use it for custom builds",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minepkg/action-install-minepkg.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "minepkg",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/io": "^1.1.0",
    "@actions/tool-cache": "^1.6.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^7.17.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^25.3.4",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.0.0",
    "prettier": "2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.2.4"
  }
}
