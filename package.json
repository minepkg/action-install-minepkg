{
  "name": "action-install-minepkg",
  "version": "0.0.1",
  "private": true,
  "description": "Installs the minepkg CLI, so you can use it for custom builds",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minepkg/action-install-minepkg.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "minepkg",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.0",
    "@actions/io": "^1.1.0",
    "@actions/tool-cache": "^1.6.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.9",
    "@typescript-eslint/parser": "^5.42.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.27.0",
    "eslint-plugin-github": "^4.1.1",
    "js-yaml": "^4.0.0",
    "prettier": "2.8.1",
    "typescript": "^4.1.3"
  }
}